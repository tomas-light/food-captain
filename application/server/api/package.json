{
  "name": "@food-captain/api",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "version": "0.0.0",
  "scripts": {
    "start": "run-s \"scrab-env\" start-app",
    "scrab-env": "ts-node build-scripts/scrab-envs.ts",
    "start-app": "cross-env NODE_ENV=development ts-node src/startApi.ts",
    "-": "",
