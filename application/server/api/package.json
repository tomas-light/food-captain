    "cheap-di": "4.0.0-rc-11",
    "cookie": "0.6.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2",
    "multer": "1.4.5-lts.1",
    "mvc-middleware": "3.0.0-alpha.2",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "rimraf": "5.0.5",
    "@types/cookie": "0.6.0",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/multer": "1.4.11",
    "@types/node": "20.10.1",
    "cross-env": "7.0.3",
